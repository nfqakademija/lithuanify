<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}{{ "Lithuanify" }}{% endblock %}</title>
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico')}}">
    {% block stylesheets %}
        <link rel="stylesheet"
              href="{{ asset('css/style.css') }}" type="text/css" >

    {% endblock %}
    {% block canonical_meta %}{% endblock %}
</head>
<body>
<div class="container">
    {% block container %}
        {% block header %}
            {% include "::inc/header.html.twig" %}
        {% endblock %}
        {% block body %}
        {% endblock %}
    {% endblock %}
</div>
{% block footer %}
{% include "::inc/footer.html.twig" %}
{% endblock %}

{% block javascripts %}
    <script>
        var test = {{ articles|json_encode|raw }};
    </script>
    <script src="{{ asset('js/script.js') }}"></script>
    <script async defer
            src='https://maps.googleapis.com/maps/api/js?key=AIzaSyDBNALb8hqG3FKicI_mEBL_SomBzrn57NI
                    &libraries=visualization&callback=initMap'>
    </script>
{% endblock %}
</body>
</html>