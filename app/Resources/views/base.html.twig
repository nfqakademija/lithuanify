<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}{{ "Lithuanify" }}{% endblock %}</title>
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico')}}">
    {% block stylesheets %}
        <link rel="stylesheet"
              href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet"
              href="{{ asset('assets/vendor/sidebar/main.css') }}">
        <link rel="stylesheet"
              href="{{ asset('assets/vendor/slider/css/iThing.css') }}" type="text/css" >
        <link rel="stylesheet"
              href="{{ asset('css/main.css') }}" type="text/css" >
        <link rel="stylesheet"
              href="{{ asset('assets/vendor/bootstrap-datepicker/css/bootstrap-datepicker3.min.css') }}" type="text/css" >
    {% endblock %}
    {% block canonical_meta %}{% endblock %}
</head>
<body>
<div class="container">
    {% block container %}
        {% block header %}
            {% include "::inc/header.html.twig" %}
        {% endblock %}
        {% block body %}
            <script async defer
                    src='https://maps.googleapis.com/maps/api/js?key=AIzaSyDBNALb8hqG3FKicI_mEBL_SomBzrn57NI
                    &libraries=visualization&callback=initMap'>
            </script>
        {% endblock %}
    {% endblock %}
</div>
{% block footer %}
{% include "::inc/footer.html.twig" %}
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('assets/vendor/jquery/dist/jquery.js') }}"></script>
    <script src="{{ asset('assets/vendor/bootstrap/dist/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('assets/vendor/slider/js/jquery-ui-1.8.16.custom.min.js') }}"></script>
    <script src="{{ asset('assets/vendor/slider/js/jQDateRangeSlider-min.js') }}"></script>
    <script src="{{ asset('js/slider.js') }}"></script>
    <script src="{{ asset('js/buttonToggle.js') }}"></script>
    <script>
        document.getElementById('date1').value = new Date().toJSON().slice(0,10);
        document.getElementById('date2').value = new Date().toJSON().slice(0,10);
    </script>
    <script src="{{ asset('assets/vendor/bootstrap-datepicker/js/bootstrap-datepicker.min.js') }}"></script>
    <script src="{{ asset('assets/vendor/bootstrap-datepicker/locales/bootstrap-datepicker.lt.min.js') }}"></script>
    <script src="{{ asset('js/datePicker.js') }}"></script>
    <script src="{{ asset('js/initMap.js') }}"></script>
{% endblock %}
</body>
</html>